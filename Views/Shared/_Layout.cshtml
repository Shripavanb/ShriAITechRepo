<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Shri AI Tech</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ShriAITech.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/Home.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.13.1/font/bootstrap-icons.min.css">
    
</head>
<body>
    <header>
        <div class="container-fluid">
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Shri AI Tech</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Products">Products & Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="ContactUs">Contact Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="AboutUs">About Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="WebDevelopment">Web Dev</a>
                        </li>
                   
                    </ul>
                </div>
         </nav>
        </div>
    </header>
    <div class="container-fluid">
        <main role="main" >
            @RenderBody()
        </main>
    </div>
    <!-----------Refrence-Row--------------->
    <div class="container-fluid referencebg">
      <div class="row" >
       @* <hr style="border-top: 5px solid darkblue"  /> *@
        <div class="container">
            <div class="row">
                 <div class="col-md-3">
                <!-----------Logo and Address ------->
                    <div class="row-cols-1">
                         <img src="~/img/CompanyTradeImage.jpg" />
                    </div>
                        <div class="row-cols-1">
                            <p><span class="glyphicon glyphicon-map-marker text-warning p-3"></span>Suchitra, PipelineRoad-500055</p>
                        <p>Hyderabad, Telangana, India</p>
                        </div>
                </div>
                <!-----------Social media------->
                <div class="col-md-3">
                    <h3>Media</h3>
                    <hr />
                    <ul>
                        <li style="display:block"><a href="#" class="icon-link"><i class="bi bi-linkedin"></i></a></li>
                            <li style="display:block"><a href="https://www.instagram.com/shriaitech?igsh=MThicHV3NnVzMWlzZA==" target="_blank" class="icon-link"><i class="bi bi-instagram"> Instagram </i></a></li>
                        <li style="display:block"><a href="#" class="icon-link"><i class="bi bi-twitter"></i></a></li>
                        <li style="display:block"><a href="#" class="icon-link"><i class="bi bi-facebook"></i></a></li>
                    </ul>
                </div>
                <!-----------Company------->
                <div class="col-md-3">
                   <h3>Company</h3> 
                   <hr />
                   <ul>
                       <li style="display:block"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                       <li style="display:block"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Products">Products & Services</a></li>
                       <li style="display:block"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></li>
                       <li style="display:block"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="ContactUs">Contact Us</a></li>
                       <li style="display:block"><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="AboutUs">About Us</a></li>
                   </ul>
                </div>
                <!-----------Contact Us------->
                <div class="col-md-3">
                   <h3>Contact</h3>
                   <hr />
                   <ul>
                            <li style="display:block"><a><span class="glyphicon glyphicon-phone p-3"></span> +91 8374499001</a></li>
                            <li style="display:block"><a><span class="glyphicon glyphicon-envelope p-3"></span> shripavanb@shriaitech.in</a></li>
                   </ul>
                    </div>
                </div>
            </div>
      </div>
    </div>
    <!---------Footer-------------->
    <footer class="border-top footer text-muted" style="background-color:white; margin-top:10px">
        <div class="container-fluid">
            &copy; 2025 - ShriAITech - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a> 
        </div>
    </footer>
    <!---------Script-------------->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
